{
  "name": "@spotter/solidstart",
  "description": "Solidstart sdk for spotter",
  "version": "0.0.1",
  "publishConfig": {
    "access": "public"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "rm -rf dist && tsup ./src --format cjs,esm --dts",
    "test": "jest --passWithNoTests --detectOpenHandles --u",
    "lint": "tsc"
  },
  "files": [
    "dist/**",
    "package.json"
  ],
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    }
  },
  "keywords": [
    "observability",
    "solidstart",
    "hackathon",
    "middleware",
    "sdk"
  ],
  "author": "Emmanuel Ajike",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.3.4",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.3",
    "node-os-utils": "^1.3.7",
    "stacktracey": "^2.1.8"
  },
  "devDependencies": {
    "@solidjs/start": "^1.0.8",
    "@types/axios": "^0.14.0",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.10",
    "@types/node": "^20.6.1",
    "@types/node-os-utils": "^1.3.2",
    "@types/supertest": "^2.0.16",
    "express": "^4.18.2",
    "jest": "^29.7.0",
    "nodemon": "^2.0.20",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "tsup": "^7.2.0",
    "typescript": "^5.1.6"
  }
}
