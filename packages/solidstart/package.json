{
  "name": "@spotter.dev/solidstart",
  "description": "Solidstart SDK for spotter.",
  "version": "0.0.3",
  "publishConfig": {
    "access": "public"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "rm -rf dist && tsup ./src --format cjs,esm --dts",
    "test": "jest --passWithNoTests --detectOpenHandles --u",
    "lint": "tsc --noEmit"
  },
  "files": [
    "dist/**",
    "README.md",
    "package.json"
  ],
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    }
  },
  "keywords": [
    "observability",
    "solidstart",
    "hackathon",
    "middleware",
    "sdk"
  ],
  "author": {
    "email": "emmanuelajike2000@gmail.com",
    "name": "Emmanuel Ajike",
    "url": "https://github.com/emee-dev"
  },
  "license": "MIT",
  "dependencies": {
    "axios": "^1.3.4",
    "dotenv": "^16.0.3",
    "node-os-utils": "^1.3.7",
    "normalize-url": "^8.0.1",
    "qs": "^6.13.0",
    "request-ip": "^3.3.0",
    "stack-trace": "1.0.0-pre2",
    "stacktracey": "^2.1.8",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@solidjs/start": "^1.0.8",
    "@spotter.dev/spotter_types": "workspace:*",
    "@types/axios": "^0.14.0",
    "@types/node": "^20.6.1",
    "@types/node-os-utils": "^1.3.2",
    "@types/qs": "^6.9.17",
    "@types/request-ip": "^0.0.41",
    "@types/stack-trace": "^0.0.33",
    "ts-jest": "^29.1.1",
    "tsconfig-paths": "^4.2.0",
    "tsup": "^7.2.0",
    "typescript": "^5.1.6"
  }
}
